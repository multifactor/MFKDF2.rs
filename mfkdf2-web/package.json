{
  "name": "mfkdf2-web",
  "version": "0.1.0",
  "description": "MFKDF2 bindings for web",
  "author": "Multifactor",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git://github.com/multifactor/mfkdf2.rs.git"
  },
  "scripts": {
    "ubrn:web": "ubrn build web --config .config/ubrn.config.yaml",
    "ubrn:web:release": "CARGO_TARGET_WASM32_UNKNOWN_UNKNOWN_RUSTFLAGS='-C opt-level=3 -C codegen-units=1 -C panic=abort -C target-feature=+simd128' ubrn build web --release --config .config/ubrn.config.yaml",
    "ubrn:web:differential": "ubrn build web --config .config/ubrn-differential.config.yaml",
    "ubrn:web:differential:release": "CARGO_TARGET_WASM32_UNKNOWN_UNKNOWN_RUSTFLAGS='-C opt-level=3 -C codegen-units=1 -C panic=abort -C target-feature=+simd128' ubrn build web --release --config .config/ubrn-differential.config.yaml",
    "wasm:opt": "wasm-opt -O3 --enable-simd -o src/generated/web/wasm-bindgen/index_bg.opt.wasm src/generated/web/wasm-bindgen/index_bg.wasm",
    "ubrn:checkout": "ubrn checkout",
    "ubrn:clean": "rm -rfv cpp/ android/CMakeLists.txt android/src/main/java android/*.cpp ios/ src/Native* src/*.web.ts* src/generated/",
    "typecheck": "tsc",
    "test": "npx tsx node_modules/mocha/bin/mocha --ignore test/differential/**/*.test.ts",
    "test:report": "npx tsx node_modules/mocha/bin/mocha --ignore test/differential/**/*.test.ts --reporter mocha-multi-reporters --reporter-options configFile=.config/mocha-reporters.json",
    "test:watch": "npx tsx node_modules/mocha/bin/mocha --watch",
    "test:differential": "npx tsx node_modules/mocha/bin/mocha -- -g 'differential'",
    "test:differential:report": "npx tsx node_modules/mocha/bin/mocha -- -g 'differential' --reporter mocha-multi-reporters --reporter-options configFile=.config/mocha-reporters-differential.json",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "uniffi-bindgen-react-native": "^0.29.3-1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.5.5",
    "ajv": "^8.17.1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.2.1",
    "mocha-multi-reporters": "^1.5.1",
    "mochawesome": "^7.1.3",
    "prettier": "^3.6.2",
    "speakeasy": "^2.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "mfkdf": "github:multifactor/MFKDF#3d5bf7"
  }
}